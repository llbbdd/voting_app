<!DOCTYPE html>
<html>
    <head>
        <% include ../public/partials/head %>
    </head>
    
    <body>
        <% include ../public/partials/header %>
        
        <div class="container">
            <%= poll[0].pollname %></br>
            <% for(var i=0; i<poll[0].polloptions.length; i++){ %>
                <%= poll[0].polloptions[i]%> <%= Math.floor((poll[0]["option" + i] / poll[0].totalvotes) * 100) %>%</br>
            <% } %>
        </div>
        
        <% include ../public/partials/footer %>
    </body>
</html>